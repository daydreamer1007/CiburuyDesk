<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>

<?import javafx.stage.Screen?>
<Pane id="welcomePane" fx:id="welcomePane" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.WelcomeController">
   <children>
       <ImageView fx:id="logo" preserveRatio="true" smooth="true">
           <image>
               <Image url="/logo.png" />
           </image>
       </ImageView>
       <TextField fx:id="username" promptText="Username" />
       <PasswordField fx:id="password" layoutX="387.0" layoutY="284.0" prefHeight="31.0" prefWidth="207.0" promptText="Password" />
       <Button id="button-styles" fx:id="login" layoutX="387.0" layoutY="348.0" mnemonicParsing="false" prefHeight="47.0" prefWidth="207.0" stylesheets="@style.css" text="Login" textFill="WHITE" textOverrun="CLIP" />
       <Button id="button-styles" fx:id="guest" layoutX="387.0" layoutY="410.0" mnemonicParsing="false" prefHeight="47.0" prefWidth="207.0" stylesheets="@style.css" text="Be a Guest" textFill="WHITE" />
   </children>
</Pane>
